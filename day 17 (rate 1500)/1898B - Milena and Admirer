import sys,math
input=sys.stdin.readline

t=int(input())
for _ in range(t):
    n=int(input())
    a=list(map(int,input().split()))
    ans=0
    cur=a[-1]
    for i in range(n-2,-1,-1):
        if a[i]<=cur:
            cur=a[i]
        else:
            parts=(a[i]+cur-1)//cur
            ans+=parts-1
            cur=a[i]//parts
    print(ans)
