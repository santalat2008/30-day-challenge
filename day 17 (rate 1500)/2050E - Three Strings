t=int(input())
for _ in range(t):
    a=input().strip()
    b=input().strip()
    c=input().strip()
    n,m=len(a),len(b)
    dp=[[10**9]*(m+1) for _ in range(n+1)]
    dp[0][0]=0
    for i in range(n+1):
        for j in range(m+1):
            if i<n:
                dp[i+1][j]=min(dp[i+1][j],dp[i][j]+(a[i]!=c[i+j]))
            if j<m:
                dp[i][j+1]=min(dp[i][j+1],dp[i][j]+(b[j]!=c[i+j]))
    print(dp[n][m])
