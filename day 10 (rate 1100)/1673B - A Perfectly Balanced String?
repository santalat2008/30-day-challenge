t = int(input())
for _ in range(t):
    s = input()
    ok = True
    i = 0
    while i < len(s):
        j = i
        while j < len(s) and s[j] == s[i]:
            j += 1
        if j < len(s) and s[j] == s[i - 1 if i > 0 else j]:
            ok = False
            break
        i = j
    print("YES" if ok else "NO")
