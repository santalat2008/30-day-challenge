import sys,bisect
input=sys.stdin.readline
t=int(input())
for _ in range(t):
 n,q=map(int,input().split())
 a=list(map(int,input().split()))
 k=list(map(int,input().split()))
 m=[0]*n
 m[0]=a[0]
 for i in range(1,n):m[i]=max(m[i-1],a[i])
 p=[0]*(n+1)
 for i in range(1,n+1):p[i]=p[i-1]+a[i-1]
 for x in k:
  i=bisect.bisect_right(m,x)
  print(p[i],end=' ')
 print()
