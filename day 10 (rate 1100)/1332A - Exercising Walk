for _ in range(int(input())):
    l, r, d, u = map(int, input().split())
    x, y, x1, y1, x2, y2 = map(int, input().split())
    if (r - l > x2 - x1 and x1 == x2) or (u - d > y2 - y1 and y1 == y2):
        print("No")
        continue
    x += r - l
    y += u - d
    ok = x1 <= x <= x2 and y1 <= y <= y2
    print("Yes" if ok else "No")
