n,m=map(int,input().split())
a=list(map(int,input().split()))
N=2**n
t=[0]*(2*N)
for i in range(N):t[N+i]=a[i]
def bld():
    for i in range(N-1,0,-1):
        h=(i.bit_length()-1)
        t[i]=t[i<<1]|t[i<<1|1] if h%2==0 else t[i<<1]^t[i<<1|1]
bld()
for _ in range(m):
    p,v=map(int,input().split())
    i=p-1+N
    t[i]=v
    i>>=1
    while i:
        h=(i.bit_length()-1)
        t[i]=t[i<<1]|t[i<<1|1] if h%2==0 else t[i<<1]^t[i<<1|1]
        i>>=1
    print(t[1])
