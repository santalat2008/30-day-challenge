import sys
import math

def f(x):
    r=0
    d=2
    while d*d<=x:
        while x%d==0:
            r+=1
            x//=d
        d+=1
    if x>1:r+=1
    return r

t=int(sys.stdin.readline())
for _ in range(t):
    a,b,k=map(int,sys.stdin.readline().split())
    g=math.gcd(a,b)
    mn=0
    if a!=b: mn=1
    mx=f(a)+f(b)
    if k<mn or k>mx:print("NO")
    elif k==1 and (a%b!=0 and b%a!=0):print("NO")
    else:print("YES")
