import sys
input=sys.stdin.readline

t=int(input())
for _ in range(t):
    n=int(input())
    a=input().strip()
    b=input().strip()
    r=[]
    for i in range(n-1,-1,-1):
        if a[0]!=b[i]:
            r.append(1)
            a=('1' if a[0]=='0' else '0')+a[1:]
        r.append(i+1)
        a=a[:i+1][::-1].translate(str.maketrans('01','10'))+a[i+1:]
    print(len(r),*r)
