g = [[1]*3 for _ in range(3)]
for i in range(3):
    a = list(map(int, input().split()))
    for j in range(3):
        if a[j] % 2:
            for dx, dy in [(0, 0), (1, 0), (-1, 0), (0, 1), (0, -1)]:
                x, y = i + dx, j + dy
                if 0 <= x < 3 and 0 <= y < 3:
                    g[x][y] ^= 1
for r in g:
    print(*r, sep='')
