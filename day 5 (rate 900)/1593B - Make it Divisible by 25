for _ in range(int(input())):
    s = input()
    n = len(s)
    ans = n + 1
    for p in ['00', '25', '50', '75']:
        i = n - 1
        j = 1
        for c in reversed(p):
            while i >= 0 and s[i] != c:
                i -= 1
                j += 1
            if i < 0:
                break
            i -= 1
        else:
            ans = min(ans, j - 1)
    print(ans)
