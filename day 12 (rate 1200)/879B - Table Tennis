n, k = map(int, input().split())
a = list(map(int, input().split()))
if k >= n:
    print(max(a))
else:
    cur = a[0]
    win = 0
    for i in range(1, n):
        if cur > a[i]:
            win += 1
        else:
            cur = a[i]
            win = 1
        if win == k:
            break
    print(cur)
