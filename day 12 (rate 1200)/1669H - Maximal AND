t = int(input())
for _ in range(t):
    n, k = map(int, input().split())
    arr = list(map(int, input().split()))

    ans = 0
    for bit in range(30, -1, -1):
        cost = 0
        mask = 1 << bit
        for x in arr:
            if (x & mask) == 0:
                cost += 1
        if cost <= k:
            k -= cost
            ans |= mask
    print(ans)
