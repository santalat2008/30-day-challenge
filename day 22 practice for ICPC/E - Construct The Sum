t = int(input())
for _ in range(t):
    n, s = map(int, input().split())
    if s > n * (n + 1) // 2:
        print(-1)
        continue
    res = []
    for i in range(n, 0, -1):
        if s >= i:
            res.append(i)
            s -= i
        if s == 0:
            break
    if s == 0:
        print(len(res), *res[::-1])
    else:
        print(-1)
