s = list(input().strip())
n = len(s)
ok = True
for i in range(n // 2):
    if s[i] == s[n - 1 - i] == '?':
        s[i] = s[n - 1 - i] = 'a'
    elif s[i] == '?':
        s[i] = s[n - 1 - i]
    elif s[n - 1 - i] == '?':
        s[n - 1 - i] = s[i]
    elif s[i] != s[n - 1 - i]:
        ok = False
        break
if not ok:
    print(-1)
else:
    if n % 2 == 1 and s[n // 2] == '?':
        s[n // 2] = 'a'
    print("".join(s))
