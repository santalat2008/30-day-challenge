t = int(input())
for _ in range(t):
    n, m = map(int, input().split())
    a = [input().strip() for _ in range(n)]
    if a[0][0] == '1':
        print(-1)
        continue
    ops = []
    for i in range(n-1, -1, -1):
        for j in range(m-1, -1, -1):
            if a[i][j] == '1':
                if j > 0:
                    ops.append((i+1, j, i+1, j+1))
                else:
                    ops.append((i, j+1, i+1, j+1))
    print(len(ops))
    for op in ops:
        print(*op)
