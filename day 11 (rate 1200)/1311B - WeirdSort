t = int(input())
for _ in range(t):
    n, m = map(int, input().split())
    a = list(map(int, input().split()))
    p = list(map(int, input().split()))
    can = [0] * (n - 1)
    for x in p:
        can[x - 1] = 1

    i = 0
    while i < n:
        j = i
        while j < n - 1 and can[j]:
            j += 1
        a[i:j+1] = sorted(a[i:j+1])
        i = j + 1

    print("YES" if all(a[i] <= a[i+1] for i in range(n - 1)) else "NO")
