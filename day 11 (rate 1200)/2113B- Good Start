t = int(input())
for _ in range(t):
    w, h, a, b = map(int, input().split())
    x1, y1, x2, y2 = map(int, input().split())
    ok = False
    if x1 != x2 and (x1 - x2) % a == 0: ok = True
    if y1 != y2 and (y1 - y2) % b == 0: ok = True
    if (x1 - x2) % a == 0 and (y1 - y2) % b == 0: ok = True
    print("Yes" if ok else "No")
