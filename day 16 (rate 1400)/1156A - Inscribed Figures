n=int(input())
a=list(map(int,input().split()))
s=0
ok=True
for i in range(1,n):
    if {a[i-1],a[i]}=={2,3}:
        print("Infinite")
        ok=False
        break
    if {a[i-1],a[i]}=={1,2}:
        s+=3
    else:
        s+=4
    if i>1 and a[i-2:i+1]==[3,1,2]:
        s-=1
if ok:
    print("Finite\n"+str(s))
