t=int(input())
for _ in range(t):
    n=int(input())
    if n<4: 
        print(-1)
        continue
    if n==4:
        print("3 1 4 2")
        continue
    res=[]
    for i in range(n-(n%2==1),0,-2):
        res.append(i)
    if n%2==1:
        res+=[2,4,1,3]+list(range(5,n,2))
    else:
        res+=list(range(2,n+1,2))
    print(*res)
