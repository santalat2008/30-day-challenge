t = int(input())
for _ in range(t):
    x, y = map(int, input().split())
    if x >= y:
        print("YES")
    elif x >= 4:
        print("YES")
    elif x == 3 and y <= 3:
        print("YES")
    elif x == 2 and y <= 3:
        print("YES")
    elif x == 1 and y == 1:
        print("YES")
    else:
        print("NO")
