t = int(input())
for _ in range(t):
    s = list(input())
    n = len(s)
    for i in range(n):
        if s[i] != '?':
            for j in range(i - 1, -1, -1):
                if s[j] == '?':
                    s[j] = s[j + 1]
                else:
                    break
            break
    for i in range(1, n):
        if s[i] == '?':
            s[i] = s[i - 1]
    
    print("".join(s))
