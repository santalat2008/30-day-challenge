t = int(input())
for _ in range(t):
    n = int(input())
    a = input()
    b = input()

    possible = True
    for i in range(n):
        if a[i] == '1':
            if (i > 0 and b[i-1] == '0') or (i < n-1 and b[i+1] == '0'):
                continue 
            elif b[i] == '0':
                continue
            elif i > 0 and a[i-1] == '0':
                continue 
            else:
                possible = False
                break

    print("YES" if possible else "NO")
