t = int(input())
for _ in range(t):
    n = int(input())
    a = list(map(int, input().split()))
    b = list(map(int, input().split()))

    pos_a = [0] * (n + 1)
    pos_b = [0] * (n + 1)

    for i in range(n):
        pos_a[a[i]] = i
        pos_b[b[i]] = i

    min_pos = float('inf')
    answer = 'Alice'

    for x in range(1, n + 1):
        if pos_a[x] + pos_b[x] <= n - 1:
            answer = 'Bob'
            break

    print(answer)
