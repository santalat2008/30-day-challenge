from math import gcd
t = int(input())
for _ in range(t):
    l, r = map(int, input().split())
    f = 0
    for x in range(l, r + 1):
        for y in range(1, x):
            if gcd(x - y, y) > 1:
                print(x - y, y)
                f = 1
                break
        if f: break
    if not f: print(-1)
