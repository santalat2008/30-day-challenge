from math import isqrt
t = int(input())
for _ in range(t):
    a, b = map(int, input().split())
    if a == b:
        print(2 * (a - 1) + 1)
        continue
    d = abs(a - b)
    r = 0
    for i in range(1, isqrt(d) + 1):
        if d % i == 0:
            r += (i != d // i) * 2 + 2
    print(r)
