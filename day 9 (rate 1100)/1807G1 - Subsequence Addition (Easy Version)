t = int(input())
for _ in range(t):
    n = int(input())
    c = list(map(int, input().split()))
    c.sort()
    if c[0] != 1:
        print("NO")
        continue
    total = 1
    ok = True
    for i in range(1, n):
        if c[i] > total:
            ok = False
            break
        total += c[i]
    print("YES" if ok else "NO")
