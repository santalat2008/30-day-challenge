t = int(input())
for _ in range(t):
    n, q = map(int, input().split())
    d = [0] * (n + 2)
    for _ in range(q):
        l, r = map(int, input().split())
        d[l] += 1
        d[r + 1] -= 1
    for i in range(1, n + 1):
        d[i] += d[i - 1]
    print(''.join(['1' if x % 2 else '0' for x in d[1:n + 1]]))
