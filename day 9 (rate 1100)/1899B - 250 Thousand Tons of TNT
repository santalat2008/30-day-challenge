t = int(input())
for _ in range(t):
    n = int(input())
    a = list(map(int, input().split()))
    r = 0
    for d in range(1, n + 1):
        if n % d: continue
        for i in range(d):
            mx = mn = a[i]
            for j in range(i, n, d):
                mx = max(mx, a[j])
                mn = min(mn, a[j])
            r = max(r, mx - mn)
    print(r)
