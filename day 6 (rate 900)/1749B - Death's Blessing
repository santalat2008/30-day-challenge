t = int(input())
for _ in range(t):
    n = int(input())
    a = list(map(int, input().split()))
    b = list(map(int, input().split()))
    s = 0
    for i in range(n):
        p = (i - 1 + n) % n
        a[i] = min(a[i], b[p])
    m = 10**18
    for i in range(n):
        p = (i - 1 + n) % n
        m = min(m, a[i] - min(a[i], b[p]))
    print(sum(a) - m)
